    "license": "UNLICENSED",
    "private": true,
    "engines": {
        "node": ">=20.0"
    },
    "scripts": {
        "lint": "biome check --config-path ../../biome.json ../..",
        "build-ts": "tsc -p tsconfig.build.json",
        "build": "yarn -s build-ts && yarn -s lint",
        "start:api": "sh -c 'node --watch --watch-preserve-output dist/server.js'",
        "start": "yarn -s build-ts -w & yarn add dotenv & yarn prisma generate & yarn -s start:api",
        "test": "jest --maxWorkers=1"
    },
    "jest": {
        "preset": "ts-jest"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.4",
        "@types/express": "4.17.21",
        "@types/jest": "29.5.14",
        "@types/node": "22.10.6",
        "@types/pg": "8.11.11",
        "jest": "29.7.0",
        "prisma": "^6.3.1",
        "ts-jest": "29.2.5",
        "ts-node": "^10.9.2",
        "typescript": "5.7.3"
    },
    "dependencies": {
        "@prisma/client": "^6.3.1",
        "axios": "^1.7.9",
        "dotenv": "^16.4.7",
        "express": "4.21.2",
        "pg": "8.13.1"
    }
}
